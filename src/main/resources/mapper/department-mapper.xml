<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.DepartmentRepository">
    <resultMap type="com.example.demo.model.Department" id="DepartmentResult">
        <id property="id" column="id"/>
        <result property="departName" column="dep_name"/>
        <result property="leader" column="leader"/>
    </resultMap>
    <select id="findAll" resultType="com.example.demo.model.Department" resultMap="DepartmentResult">
        select * from dep
    </select>
</mapper>